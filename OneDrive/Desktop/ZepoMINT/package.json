{
  "name": "zepomint",
  "version": "1.0.0",
  "description": "Production sealed-bid NFT mint using Zama fhEVM v0.8",
  "main": "index.js",
  "scripts": {
    "test": "npx hardhat test",
    "deploy:sepolia": "npx hardhat run backend/scripts/deploy-zepamint-proper-improved.js --network sepolia",
    "deploy:localhost": "npx hardhat run backend/scripts/deploy-zepamint-proper-improved.js --network localhost",
    "compile": "npx hardhat compile",
    "dev:setup": "node scripts/dev-setup.js",
    "test:local": "node scripts/test-local.js"
  },
  "keywords": [
    "zama",
    "fhevm",
    "nft",
    "auction",
    "sealed-bid"
  ],
  "author": "ZepoMint Team",
  "license": "MIT",
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "hardhat": "^2.19.0",
    "vite": "^7.1.5"
  },
  "dependencies": {
    "@fhevm/hardhat-plugin": "^0.1.0",
    "@fhevm/sdk": "^0.7.0-20",
    "@fhevm/solidity": "^0.8.0",
    "@vitejs/plugin-react": "^5.0.2",
    "@zama-fhe/relayer-sdk": "^0.2.0",
    "dotenv": "^16.6.1",
    "fhevm": "^0.6.2",
    "hardhat-deploy": "^1.0.4",
    "react-router-dom": "^7.8.2",
    "vite-plugin-node-polyfills": "^0.24.0"
  }
}
